{
    "entryFile": "src/app.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs": ["src/**/*Controller.ts"],
    "spec": {
      "outputDirectory": "build",
      "specVersion": 3,
      "securityDefinitions": {
      "jwt": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "Entrez votre token JWT"
      }
    }
    },
    "routes": {
        "routesDir": "src",
        "authenticationModule": "src/auth/jwtAuth.ts"
      }
  }